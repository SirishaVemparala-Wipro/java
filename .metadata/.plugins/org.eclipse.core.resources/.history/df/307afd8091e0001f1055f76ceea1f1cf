package com.wipro.java;

class Main {
    int num = 5;
 
    public void changeValue(int num) {
        this.num = num;
    }
}
 
public class  {
    public static void main(String[] args) {
        MyClass obj = new MyClass();
        obj.changeValue(10);
        System.out.println(obj.num);
    }
}

